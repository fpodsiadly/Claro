{
  "version": 2,
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "zeroConfig": true
      }
    },
    {
      "src": "backend/api/search.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/search.py"
    },
    {
      "src": "/frontend",
      "dest": "/index.html",
      "status": 308
    },
    {
      "src": "/frontend/(.*)",
      "dest": "/$1",
      "status": 308
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "backend/api/*.py": {
      "runtime": "python3.9"
    }
  },
  "devCommand": "cd frontend && npm run dev"
}