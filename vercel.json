{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "routes": [
    {
      "src": "backend/api/(.*)",
      "dest": "backend/api/search.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "backend/api/*.py": {
      "runtime": "python3.9"
    }
  },
  "devCommand": "cd frontend && npm run dev"
}